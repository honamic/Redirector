{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Redirector": {
    "ForceLowercaseUrls": true,
    "TrailingSlash": "ForceToAppend",
    "WwwMode": "ForceToNonWww",
    "RedirectStatusCode": 307,
    "Items": [
      {
        "Id": "1",
        "Type": "Path",
        "Path": "/posts",
        "Destination": "/weblog",
        "Order": 1,
        "HttpCode": 302
      },
      {
        "Id": "1",
        "Type": "Path",
        "Path": "/stores",
        "Destination": "/",
        "Order": 1,
        "HttpCode": 410
      },
      {
        "Id": "2",
        "Type": "Regex",
        "Path": "/posts/([0-9]*)/(.*)",
        "Destination": "/weblog/$1/$2",
        "Order": 2,
        "HttpCode": null
      }

    ]
  }
}
